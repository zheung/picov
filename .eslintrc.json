{
	"env": {
		"node": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 8
	},
	"rules": {
		"indent": [ 1, "tab" ],
		"linebreak-style": [ 2, "windows" ],
		"quotes": [ 2, "single" ],
		"semi": [ 2, "always" ],
		"no-unused-vars": [ 1, { "vars": "all", "args": "after-used" } ],
		"no-console": [ 1 ]
	},
	"globals": {
		"L": true,
		"soger": true,

		"_dr": true,

		"fs": true,
		"path": true,
		"qs": true,
		"url": true,
		"http": true,
		"https": true,

		"Promise": true,
		"jsdom": true,
		"request": true,

		"io": true,
		"db": true,

		"conf": true,
		"func": true,
		"work": true,
		"cache": true
	}
}