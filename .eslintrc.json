{
	"env": {
		"node": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "script"
	},
	"rules": {
		"indent": [ 2, "tab", { "ignoreComments": true, "SwitchCase": 1 } ],
		"linebreak-style": [ 2, "windows" ],
		"quotes": [ 2, "single" ],
		"semi": [ 2, "always" ],
		"no-unused-vars": [ 2, { "vars": "all", "args": "after-used" } ],
		"no-console": [ 2 ],
		"no-var": [ 2 ]
	},
	"globals": {
		"_fs": true,
		"_pa": true,
		"_qs": true,
		"_ul": true,
		"_os": true,
		"_cr": true,

		"G": true,

		"J": true,
		"JC": true,
		"JD": true,

		"L": true,
		"LE": true,

		"E": true,
		"P": true
	}
}