{
	"env": {
		"browser": true,
		"es6": false
	},
	"extends": "eslint:recommended",
	"rules": {
		"indent": [ 1, "tab" ],
		"linebreak-style": [ 2, "windows" ],
		"quotes": [ 2, "single" ],
		"semi": [ 2, "always" ],
		"no-unused-vars": [ 1, { "vars": "all", "args": "after-used" } ],
		"no-console": [ 0 ]
	},
	"globals": {
		"$": true,

		"d": true
	}
}