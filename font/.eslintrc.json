{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module"
	},
	"rules": {
		"indent": [ 2, "tab", { "ignoreComments": true, "SwitchCase": 1 } ],
		"linebreak-style": [ 2, "windows" ],
		"quotes": [ 2, "single" ],
		"semi": [ 2, "always" ],
		"no-unused-vars": [ 2, { "vars": "all", "args": "after-used" } ],
		"no-console": [ 2 ],
		"no-var": [ 2 ]
	},
	"globals": {
		"Vue": true,
		"io": true,
		"moment": true,

		"L": true,
		"C": true,
		"A": true,

		"BUS": true,
		"X": true,
		"W": true,

		"app": true,

		"System": true
	}
}